{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npx prisma generate && npx prisma migrate deploy && cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "npx prisma generate",
    "deploy:clean": "powershell -Command \"Remove-Item -Recurse -Force .nuxt, node_modules\\.cache, .output -ErrorAction SilentlyContinue\"",
    "deploy:prepare": "npm run deploy:clean && npx prisma generate && npm run build",
    "vercel-build": "npm install && npx prisma generate && npx prisma migrate deploy && cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" nuxt build",
    "seed": "node scripts/seed-database.js"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.9.0",
    "@nuxt/eslint": "^1.4.1",
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/image": "^1.10.0",
    "@nuxt/ui": "^2.14.1",
    "@nuxtjs/robots": "^5.4.0",
    "@nuxtjs/sitemap": "^7.4.3",
    "@nuxtjs/tailwindcss": "^6.12.1",
    "@pinia/nuxt": "^0.11.1",
    "@prisma/client": "^6.12.0",
    "@tailwindcss/postcss": "^4.1.11",
    "bcryptjs": "^3.0.2",
    "eslint": "^9.29.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-vue-next": "^0.522.0",
    "nuxt": "^3.12.3",
    "pinia": "^3.0.3",
    "prisma": "^6.12.0",
    "typed.js": "^2.1.0",
    "vue": "^3.5.16",
    "vue-router": "^4.5.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11"
  }
}
